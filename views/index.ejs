<!DOCTYPE html>
<html>
    <head>
        <meta charset="utf-8"/>
        <title>File Server</title>
        <link rel="shortcut icon" href="<%= STATICFILES %>/favicon.ico">
        <script src="/socket.io/socket.io.js"></script>
        <script src="<%= STATICFILES %>/index.js"></script>
    </head>
    <body>
        <h1><%= PATH %></h1>
        <% if(PARENT) { %>
        <a href="<%= PARENT %>">back</a>
        <% } %>
        <% if(ERROR) { %>
        <h2><%= ERROR %></h2>
        <% } else { %>
        <ul>
            <% FILES.forEach( FILE => { %>
            <li><a href="<%= FILE %>"><%= FILE %></a></li>
            <% }) %>
        </ul>
        <% } %>
    </body>
</html>
